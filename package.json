{
  "name": "@birdcar/markdown",
  "version": "0.0.1",
  "description": "Birdcar Flavored Markdown (BFM) parser and renderer â€” unified/remark plugins",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./tasks": {
      "types": "./dist/inlines/tasks/index.d.ts",
      "import": "./dist/inlines/tasks/index.js"
    },
    "./modifiers": {
      "types": "./dist/inlines/modifiers/index.d.ts",
      "import": "./dist/inlines/modifiers/index.js"
    },
    "./mentions": {
      "types": "./dist/inlines/mentions/index.d.ts",
      "import": "./dist/inlines/mentions/index.js"
    },
    "./directives": {
